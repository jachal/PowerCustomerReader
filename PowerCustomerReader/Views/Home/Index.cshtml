@model OrganisationsViewModel
@{
    ViewData["Title"] = "Filer";
}

<div class="conatiner">
    <div class="row justify-content-center">
        <div class="col-lg-5 justify-content-center">
            <form asp-controller="Home" asp-action="Index" method="post" enctype="multipart/form-data">
                <div class="input-group">
                    <input type="file" name="formFile" class="form-control" id="inputGroupFile04" aria-describedby="inputGroupFileAddon04" aria-label="Upload">
                    <button class="btn btn-outline-secondary" type="submit" id="inputGroupFileAddon04">Les filen</button>
                </div>
            </form>
        </div>
    </div>
    <div class="row pt-4 justify-content-center">
        <a class="btn btn-outline-primary" asp-area="" asp-controller="Home" asp-action="Purge">Slett alle filer</a>
        <br />
    </div>
    <div class="row justify-content-center">
        @if (Model.Organisations != null)
        {
            <p>Organisasjoner lest fra brønnesunderegisteret</p>
        }
    </div>
</div>

